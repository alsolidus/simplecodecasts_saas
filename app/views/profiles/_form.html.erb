<%= form_for @profile, url: user_profile_path, :html => { :multipart => true } do |f| %>
                <div class="form-group">
                    <%= f.label :first_name %>
                    <%= f.text_field :first_name, class: 'form-control' %> 
                </div>
                <div class="form-group">
                    <%= f.label :last_name %>
                    <%= f.text_field :last_name, class: 'form-control' %> 
                </div>
                <div class="form-group">
                    <%= f.label :avatar %>
                    <%= f.file_field :avatar, class: 'form-control' %> 
                </div>
                <div class="form-group">
                    <%= f.label :job_title %>
                    <%= f.select :job_title, ['NOC','ECC','Legacy', 'CHS14'], {}, class: 'form-control' %> 
                </div>
                <div class="form-group">
                    <%= f.label :phone_number %>
                    <%= f.text_field :phone_number, class: 'form-control' %> 
                </div>
                <div class="form-group">
                    <%= f.label :alt_number %>
                    <%= f.text_field :alt_number, class: 'form-control' %> 
                </div>
                <div class="form-group">
                    <%= f.label :hosptial_name %>
                    <%= f.text_field :hosptial_name, class: 'form-control' %> 
                </div>
                <div class="form-group">
                    <%= f.label :site_code %>
                    <%= f.text_field :site_code, class: 'form-control' %> 
                </div>
                <div class="form-group">
                    <%= f.label :description %>
                    <%= f.text_area :description, class: 'form-control' %> 
                </div>
                <div =class"form-group">
                    <%= f.submit "Update Profile", class: 'btn btn-primary' %>
                </div>
            <% end %>